{
  "name": "@geostrategists/react-router-sessions-dynamodb",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geostrategists/react-router-sessions-dynamodb.git"
  },
  "exports": {
    ".": {
      "node": {
        "types": "./dist/index.d.ts",
        "module-sync": "./dist/index.mjs",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "build": "tsc && tsup",
    "prepack": "yarn build && yarn npmignore --auto",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "react-router": "^7.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.767.0",
    "@aws-sdk/lib-dynamodb": "^3.767.0",
    "@eslint/js": "^9.22.0",
    "@semantic-release/exec": "^7.0.3",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "npmignore": "^0.3.1",
    "prettier": "3.5.3",
    "react-router": "^7.3.0",
    "semantic-release": "^24.2.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "packageManager": "yarn@4.7.0+sha256.293632d8a095d8ea4786eb2c5798c83c37544abed17ed31186a3ec4549a07c06",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "verifyConditionsCmd": "echo //npm.pkg.github.com/:_authToken=${process.env.GITHUB_TOKEN} > /tmp/github.npmrc && npm whoami --userconfig /tmp/github.npmrc --registry https://npm.pkg.github.com/",
          "publishCmd": "npm publish --userconfig /tmp/github.npmrc --tag ${nextRelease.channel} --registry https://npm.pkg.github.com/ --no-git-tag-version",
          "successCmd": "rm /tmp/github.npmrc",
          "failCmd": "rm /tmp/github.npmrc"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "ignore": [
      "!dist/**",
      "src/",
      ".*",
      "tsconfig.json",
      "tsup.config.ts"
    ],
    "access": "public",
    "provenance": true
  },
  "lint-staged": {
    "*.{ts,tsx,js,cjs,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(*.ts|*.tsx|*.js|*.cjs|*.mjs)": "prettier --ignore-unknown --write"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all"
  }
}
